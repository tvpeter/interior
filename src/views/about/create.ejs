<!DOCTYPE html>
<html>
<% include ../partials/adminheader %>    
    
    <section class="page-info">
        <div class="auto-container clearfix">
            <div class="pull-left"><h2>About Us</h2></div>
        </div>
    </section>
    
    <!--Sidebar Page-->
    <div class="sidebar-page-container">
        <div class="auto-container">
            <div class="row clearfix">
                
                <!--Content Side-->      
                <div class="content-side col-lg-9 col-md-8 col-sm-12 col-xs-12">
                    
                    <!--News Section-->
                    <section class="news-section large-image-view blog-details no-padd-top no-padd-bottom">
                        
                        <div class="sec-title medium"><h2>About us Story</h2></div>
                        <p>List all the qualities of the company one at a time.</p>
                            <p><%= pageDetails.error %></p>
                        <div class="comment-form">
                        	<!--form-box-->
                            <div class="form-box default-form">
                                <div class="contact-form default-form">
                                    <form method="post" action="/about/story" enctype="multipart/form-data">
                                        <div class="row clearfix">
                                            
                                            
                                            <div class="form-group col-md-12 col-sm-12 col-xs-12">
                                                <textarea name="description" placeholder="Notes "></textarea>
                                            </div>
                                            <div class="form-group col-md-12 col-sm-12 col-xs-12">
                                                <input type="file" name="aboutimg">
                                            </div>
                                           
                                            <div class="form-group col-md-12 col-sm-12 col-xs-12">
                                                <button type="submit" class="theme-btn btn-style-two">Post</button>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
            
                        <!--News Style One / Boxed-->
                        <div class="news-style-one boxed">
                            <div class="inner-box">
                                <figure class="image-box"></figure>
                           		<div class="content">
                                   
                                    <div class="text">
                                        <%for (let quality of qualities) {%>
                                        <h4><strong><%= quality.title %></strong></h4>
                                    	<p><%= quality.description %></p>
                                        <%}%>
                                    </div>
                                </div>
                            </div>
                        </div>
                        
                        
                        
                        <div class="sec-title medium"><h2>Add a Quality</h2></div>
                        <p>List all the qualities of the company one at a time.</p>
                            <p><%= pageDetails.error %></p>
                        <div class="comment-form">
                        	<!--form-box-->
                            <div class="form-box default-form">
                                <div class="contact-form default-form">
                                    <form method="post" action="/about/create">
                                        <div class="row clearfix">
                                            
                                            <div class="form-group col-md-12 col-sm-12 col-xs-12">
                                                <input type="text" name="title" value="" placeholder="Title">
                                            </div>
                                            
                                            <div class="form-group col-md-12 col-sm-12 col-xs-12">
                                                <textarea name="description" placeholder="Notes "></textarea>
                                            </div>
                                            <div class="form-group col-md-12 col-sm-12 col-xs-12">
                                                <button type="submit" class="theme-btn btn-style-two">Post</button>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
            
                        
                    </section>
                
                </div><!--End Content Side-->   
                
                <!--Sidebar-->      
                <div class="sidebar-side col-lg-3 col-md-4 col-sm-6 col-xs-12">
                    <aside class="sidebar">
                        
                        <!-- Popular Categories -->
                        <div class="widget popular-categories sidebar-widget">
                            <div class="sidebar-title"><h3>Pages</h3></div>
                            
                            <ul class="list">
                                <li><a href="#">Texture Painting</a></li>
                                <li><a href="#">Wooden Flooring</a></li>
                                <li><a href="#">Artificial Grass Flooring</a></li>
                                <li><a href="#">Tiles &amp; Marble Works</a></li>
                            </ul>
                            
                        </div>
                        
                    </aside>
                </div><!--End Sidebar-->      
                
            </div>
        </div>
    </div>
      
    
    <% include ../partials/adminfooter %>
